FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs (
      "libsoilFoam.so"
     );

application     richardsFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         3600;

deltaT          1;

writeControl    runTime;

writeInterval   300;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

functions
{
        #include "inc_sampling_probes1"
        #include "inc_sampling_some_points"
};

