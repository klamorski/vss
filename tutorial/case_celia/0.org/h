FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      h;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField   uniform -0.615;

boundaryField
{
    frontAndBack
    {
        type    empty;
    }
    
    lowerWall
    {
        type fixedValue;
        value uniform -0.615; 
    }

    upperWall
    {
        type fixedValue;
        value uniform -0.207; 
    }
}

