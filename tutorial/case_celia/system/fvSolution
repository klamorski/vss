FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSolution;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

solvers
{
    h
    {
          solver          PCG; //7(FPCG GAMG PBiCGStab PCG PPCG PPCR smoothSolver)
          preconditioner  DIC; //6(DIC FDIC GAMG diagonal distributedDIC none)
          //smoother DICGaussSeidel; //6(DIC DICGaussSeidel FDIC GaussSeidel nonBlockingGaussSeidel symGaussSeidel)
          tolerance       1e-60;
          relTol          0.001; 
    }

    hFinal
    {
          $h
          tolerance       1e-60;
          relTol          0; 
    }
}

PISO {
    nCorrectors     1; // number of inner PISO loops ( while (pimple.correct()) { ... } )
    nNonOrthogonalCorrectors 0; // 1 means one additional loop solving the equation
    pRefCell        0;
    pRefValue       0;

    
