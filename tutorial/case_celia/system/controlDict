FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

libs (
      "libsoilFoam.so"
     );

application     soilFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

deltaT            10; 

writeInterval     360;

endTime           360; 

adjustTimeStep    false;

writeControl    runTime; 

purgeWrite      0;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      fixed;

timePrecision   2;

runTimeModifiable true;

allowSystemOperations 1;

functions
{
        probes1
        {                
                type                probes;
                libs                (sampling);        
